{
    "version": "4.2-dev4",
    "description": "A feature-packed, cross-platform game engine to create 2D and 3D games from a unified interface.",
    "homepage": "https://godotengine.org/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.2/dev4/Godot_v4.2-dev4_win64.exe.zip",
            "hash": "sha512:97e09265cf2dd5353e98d06ded3879b83e2d9fcec03eaa10d1d4dc4295ed5ced262be869e19ce55a95bbf1659fe6d1c444d8239b454225de73f4edaeb829836c"
        },
        "32bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.2/dev4/Godot_v4.2-dev4_win32.exe.zip",
            "hash": "sha512:1bdffff93a2fc2b01bbbede4a53551f539f7a3b30894db592f7396776b0713dc2ab07e2d5699e6557dbc27892a821a46942533f9dbb7cbb579cf495b9632b8cc"
        }
    },
    "pre_install": [
        "Remove-Item \"$dir\\Godot_*_console.*\"",
        "Get-Item \"$dir\\Godot_*.exe\" | Rename-Item -NewName 'godot.exe'"
    ],
    "bin": "godot.exe",
    "shortcuts": [
        [
            "godot.exe",
            "Godot Engine"
        ]
    ],
    "checkver": {
        "url": "https://godotengine.org/blog/pre-release/",
        "regex": "Dev snapshot: Godot (?<ver>[\\d.]+) dev (?<dev>[\\d.]+)",
        "replace": "${1}-dev${2}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/$matchVer/dev$matchDev/Godot_v$version_win64.exe.zip"
            },
            "32bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/$matchVer/dev$matchDev/Godot_v$version_win32.exe.zip"
            }
        },
        "hash": {
            "url": "$baseurl/SHA512-SUMS.txt"
        }
    }
}
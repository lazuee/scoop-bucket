{
    "version": "4.2-dev6",
    "description": "A feature-packed, cross-platform game engine to create 2D and 3D games from a unified interface.",
    "homepage": "https://godotengine.org/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.2/dev6/Godot_v4.2-dev6_win64.exe.zip",
            "hash": "sha512:5c63e8eee27f0daec47874e7010f82a21e2c04c7660909f1f273a76e8105e3cab3102138cd1fcd05e9fd23594c2741f2697cdb743917d0f3e03819f763ecd425"
        },
        "32bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.2/dev6/Godot_v4.2-dev6_win32.exe.zip",
            "hash": "sha512:8f62a65d91f42bff39cde89216985cfa2cb7b53b18695c31111ff2c8ebe54d15019b4b580619b7d92b64b74d3a47be9b7273bd8deb939c7c19d617b7d3b8ea19"
        }
    },
    "pre_install": [
        "Remove-Item \"$dir\\Godot_*_console.*\"",
        "Get-Item \"$dir\\Godot_*.exe\" | Rename-Item -NewName 'godot.exe'"
    ],
    "bin": "godot.exe",
    "shortcuts": [
        [
            "godot.exe",
            "Godot Engine"
        ]
    ],
    "checkver": {
        "url": "https://godotengine.org/blog/pre-release/",
        "regex": "Dev snapshot: Godot (?<ver>[\\d.]+) beta (?<beta>[\\d.]+)",
        "replace": "${1}-beta${2}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/$matchVer/dev$matchDev/Godot_v$version_win64.exe.zip"
            },
            "32bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/$matchVer/dev$matchDev/Godot_v$version_win32.exe.zip"
            }
        },
        "hash": {
            "url": "$baseurl/SHA512-SUMS.txt"
        }
    }
}
